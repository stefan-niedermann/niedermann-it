# Header add Content-Security-Policy "default-src 'self'; script-src 'self' 'unsafe-inline' https://tracking.niedermann.it/; frame-src https://tracking.niedermann.it https://www.openstreetmap.org; frame-ancestors 'none'"
Header add Strict-Transport-Security "max-age=63072000; includeSubDomains"
Header add X-Frame-Options "DENY"
Header add X-Content-Type-Options "nosniff"
Header add X-XSS-Protection "1; mode=block"
Header add Referrer-Policy "same-origin, strict-origin-when-cross-origin"
